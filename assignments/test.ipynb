    "import json\n",
    "\n",
    "filename = \"bank-holidays.json\"\n",
    "\n",
    "with open(filename,\"r\") as fn:\n",
    "    bh = json.load(fn)\n",
    "    ni_bh = bh[\"northern-ireland\"][\"events\"]\n",
    "    for row in ni_bh:\n",
    "       print (f\"{row['date']}  {row['title']}\")"
   ]
  }
